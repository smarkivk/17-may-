import java.util.Scanner;

public class Check {
    public static void main(String[] args) {
        int a; // or double 
        Scanner scanner = new Scanner(System.in);
        
        System.out.print("Enter the value of a: ");
        a = scanner.nextInt();
        
        if (a < 0) {
            System.out.println("Negative");
        } else {
            System.out.println("Positive");
        }
        
        scanner.close();
    }
}
